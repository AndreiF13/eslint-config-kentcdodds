{
  "name": "eslint-config-kentcdodds",
  "version": "0.0.0-semantically-released",
  "description": "ESLint rules according to kentcdodds' personal preference",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "eslint .",
    "find-new-rules": "eslint-find-rules --option",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kentcdodds/eslint-config-kentcdodds.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "kentcdodds",
    "javascript",
    "styleguide"
  ],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kentcdodds/eslint-config-kentcdodds/issues"
  },
  "homepage": "https://github.com/kentcdodds/eslint-config-kentcdodds#readme",
  "dependencies": {
    "eslint-plugin-import": "^1.6.1"
  },
  "peerDependencies": {
    "eslint": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "commitizen": "2.8.1",
    "cz-conventional-changelog": "1.1.6",
    "eslint": "2.8.0",
    "eslint-find-new-rules": "2.0.1",
    "eslint-find-rules": "1.7.0",
    "eslint-import-resolver-webpack": "0.2.3",
    "eslint-plugin-react": "5.0.1",
    "ghooks": "1.2.1",
    "semantic-release": "4.3.5",
    "validate-commit-msg": "2.6.1"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "npm t"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
