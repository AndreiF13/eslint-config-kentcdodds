{
  "name": "eslint-config-kentcdodds",
  "version": "0.0.0-semantically-released",
  "description": "ESLint rules according to kentcdodds' personal preference",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "eslint .",
    "find-new-rules": "eslint-find-new-rules",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kentcdodds/eslint-config-kentcdodds.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "kentcdodds",
    "javascript",
    "styleguide"
  ],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kentcdodds/eslint-config-kentcdodds/issues"
  },
  "homepage": "https://github.com/kentcdodds/eslint-config-kentcdodds#readme",
  "peerDependencies": {
    "eslint": "^2.0.0"
  },
  "devDependencies": {
    "commitizen": "2.5.0",
    "cz-conventional-changelog": "1.1.5",
    "eslint": "2.4.0",
    "eslint-find-new-rules": "1.0.0",
    "eslint-plugin-react": "3.16.1",
    "ghooks": "1.0.3",
    "semantic-release": "4.3.5",
    "validate-commit-msg": "1.1.1"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "npm t"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
